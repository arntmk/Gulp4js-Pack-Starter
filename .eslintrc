{
	"extends": "airbnb-base",
	//"parser": "@babel/eslint-parser",
	"env": {
		"browser": true
		// "jquery": true
	},
	"rules": {
		"semi": ["error", "always"],
		"quotes": ["error", "single"],
		"indent": ["error", "tab"],
		"no-tabs": ["warn", { "allowIndentationTabs": true }],
		"global-require": 0,
		"no-unused-vars": "warn",
		"no-console": "off",
		"func-names": "off",
		"no-process-exit": "off",
		"object-shorthand": "off",
		"class-methods-use-this": "off"
	}
}
