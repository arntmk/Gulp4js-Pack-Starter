{
	"name": "gulp4js-builder",
	"version": "1.2.5",
	"description": "gulp4js-builder for weblayout",
	"main": "gulpfile.js",
	"type": "module",
	"scripts": {
		"[ .git commite ]": "git config --global core.autocrlf true && git add . && git commit -m 'commite.' && git push -u origin main",
		"|0.-------------------|": "",
		"g-watch": "gulp watch",
		"[ g-development ]": "gulp default",
		"[ g-production ]": "gulp --production",
		"|1.-------------------|": "",
		"g-img": "gulp imgTsk",
		"g-svg": "gulp svgTsk",
		"g-fontgen": "gulp fontgenTsk",
		"g-font": "gulp fonTsk",
		"g-clean": "gulp clrTsk",
		"|2.-------------------|": "",
		"<[ stylelint ]>": "stylelint \"src/**/*.{css,scss}\" --ignore-path .stylelintignore --ignore-pattern \"src/scss/libs/*\" --fix",
		"<[ eslint ]>": "eslint \"src/**/*.{js,ts}\" --ignore-path .eslintignore --ignore-pattern \"src/js/libs/*\" --fix",
		"|3.-------------------|": "",
		"!!![ delete gulp ]!!!": "rm -rf node_modules",
		"!!![ install gulp ]!!!": "npm install",
		"|4.-------------------|": "",
		"check outdated": "npm outdated",
		"update dep": "npm update --save-dev && npm update --save",
		"update all": "npm update",
		"audit fix": "npm audit fix"
	},
	"browserslist": [
		"> 1%",
		"last 3 version",
		"defaults",
		"supports es6-module",
		"not dead"
	],
	"babel": {
		"presets": [
			"@babel/preset-env"
		],
		"targets": "defaults"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/arntmk/Gulp4js-builder_Starter.git"
	},
	"keywords": [
		"gulp",
		"webpack",
		"linters",
		"html/pug",
		"css/scss",
		"js/ts",
		"font",
		"img/svg",
		"svg-sprite"
	],
	"author": "OLEH SEMCHUK",
	"license": "WTFPL",
	"devDependencies": {
		"browser-sync": "^2.29.3",
		"css-loader": "^6.8.1",
		"del": "^6.1.1",
		"esbuild-loader": "^3.0.1",
		"eslint": "^8.43.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-plugin-import": "^2.27.5",
		"gulp": "^4.0.2",
		"gulp-autoprefixer": "^8.0.0",
		"gulp-cached": "^1.1.1",
		"gulp-changed": "^4.0.3",
		"gulp-clean-css": "^4.3.0",
		"gulp-dependents": "^1.2.5",
		"gulp-file-include": "^2.3.0",
		"gulp-fonter": "^0.3.0",
		"gulp-fontfacegen": "^1.0.7",
		"gulp-if": "^3.0.0",
		"gulp-imagemin": "^7.1.0",
		"gulp-newer": "^1.4.0",
		"gulp-notify": "^4.0.0",
		"gulp-plumber": "^1.2.1",
		"gulp-rename": "^2.0.0",
		"gulp-sass": "^5.1.0",
		"gulp-sass-glob": "^1.1.0",
		"gulp-ttf2woff2": "^4.0.1",
		"imagemin-pngquant": "^9.0.2",
		"imagemin-webp": "^6.0.1",
		"sass": "^1.63.6",
		"style-loader": "^3.3.3",
		"webpack": "^5.88.0",
		"webpack-stream": "^7.0.0"
	},
	"dependencies": {
		"gulp-cheerio": "^1.0.0",
		"gulp-htmlmin": "^5.0.1",
		"gulp-replace": "^1.1.4",
		"gulp-shorthand": "^1.1.0",
		"gulp-svg-sprite": "^2.0.3",
		"gulp-svgmin": "^4.1.0",
		"gulp-terser": "^2.1.0",
		"gulp-typograf": "^4.1.0",
		"gulp-version-number": "^0.2.4",
		"stylelint": "^15.9.0",
		"stylelint-config-recess-order": "^4.2.0",
		"stylelint-config-standard": "^33.0.0",
		"stylelint-config-standard-scss": "^9.0.0",
		"stylelint-declaration-block-no-ignored-properties": "^2.7.0",
		"stylelint-order": "^6.0.3",
		"stylelint-scss": "^5.0.1"
	},
	"overrides": {
		"chokidar": "3.5.3",
		"glob-parent": "6.0.2",
		"nth-check": "2.1.1",
		"fs-path": "0.0.25",
		"lodash.template": "4.5.0",
		"xmldom": "0.6.0",
		"trim-newlines": "3.0.1",
		"semver-regex": "3.1.4",
		"semver": "7.5.3"
	}
}
